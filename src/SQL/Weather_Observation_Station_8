SELECT DISTINCT(CITY) FROM STATION WHERE CITY LIKE 'A%a' OR CITY LIKE 'A%e' OR CITY LIKE 'A%i' OR CITY lIKE 'A%o' OR
CITY LIKE 'A%u' OR CITY LIKE 'E%a' OR CITY LIKE 'E%e' OR CITY LIKE 'E%i' OR CITY lIKE 'E%o' OR CITY LIKE 'E%u' OR
CITY LIKE 'I%a' OR CITY LIKE 'I%e' OR CITY LIKE 'I%i' OR CITY lIKE 'I%o' OR CITY LIKE 'I%u' OR CITY LIKE 'O%a' OR
CITY LIKE 'O%e' OR CITY LIKE 'O%i' OR CITY lIKE 'O%o' OR CITY LIKE 'O%u' OR CITY LIKE 'U%a' OR CITY LIKE 'U%e' OR
CITY LIKE 'U%i' OR CITY lIKE 'U%o' OR CITY LIKE 'U%u';